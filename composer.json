{
    "name": "test/test",
    "description": "",
    "require": {
        "php": "^8.0",
        "composer-plugin-api": "^2.0",
        "composer/composer": "^2.0",
        "psr/container": "^2.0",
        "yiisoft/config": "^1.1",
        "yiisoft/definitions": "^1.0",
        "yiisoft/injector": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Viktorprogger\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viktorprogger\\Container\\Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/config": true
        },
        "sort-packages": true
    }
}
